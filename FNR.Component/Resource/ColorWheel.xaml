<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ctrl="clr-namespace:FNR.Component">
    <Style TargetType="{x:Type TextBlock}">
        <Setter Property="FontFamily" Value="pack://application:,,,/MaterialDesignThemes.Wpf;component/Resources/Roboto/#Roboto-Bold" />
        <Setter Property="FontSize" Value="12" />
        <Setter Property="Foreground" Value="White" />
    </Style>
    <Style x:Key="ToolTipTextBlock" TargetType="{x:Type TextBlock}">
        <Setter Property="FontFamily" Value="pack://application:,,,/MaterialDesignThemes.Wpf;component/Resources/Roboto/#Roboto-Bold" />
        <Setter Property="FontSize" Value="14" />
        <Setter Property="Foreground" Value="White" />
        <Setter Property="Margin" Value="5" />
    </Style>
    <Style x:Key="ColorWheelToolTip" TargetType="{x:Type ToolTip}">
        <Setter Property="FontFamily" Value="pack://application:,,,/MaterialDesignThemes.Wpf;component/Resources/Roboto/#Roboto-Bold" />
        <Setter Property="FontSize" Value="14" />
        <Setter Property="Foreground" Value="White" />
        <Setter Property="MaxWidth" Value="170" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ToolTip}">
                    <ControlTemplate.Resources>
                        <Storyboard x:Key="ToolTipOpenStoryboard">
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ToolTipBorder" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
                                <SplineDoubleKeyFrame KeySpline="0.4,0,0.2,1"
                                                      KeyTime="0"
                                                      Value="0" />
                                <SplineDoubleKeyFrame KeySpline="0.4,0,0.2,1"
                                                      KeyTime="0:0:0.2"
                                                      Value="1" />
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ToolTipBorder" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
                                <SplineDoubleKeyFrame KeySpline="0.4,0,0.2,1"
                                                      KeyTime="0"
                                                      Value="0" />
                                <SplineDoubleKeyFrame KeySpline="0.4,0,0.2,1"
                                                      KeyTime="0:0:0.2"
                                                      Value="1" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </ControlTemplate.Resources>
                    <Border x:Name="ToolTipBorder"
                            Background="#E5616161"
                            BorderThickness="2"
                            CornerRadius="5"
                            RenderTransformOrigin="0.5,0.5">
                        <Border.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform />
                                <SkewTransform />
                                <RotateTransform />
                                <TranslateTransform />
                            </TransformGroup>
                        </Border.RenderTransform>
                        <ContentPresenter Margin="7,3"
                                          TextElement.FontFamily="pack://application:,,,/MaterialDesignThemes.Wpf;component/Resources/Roboto/#Roboto-Bold"
                                          TextElement.FontSize="14"
                                          TextElement.Foreground="White" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <EventTrigger RoutedEvent="FrameworkElement.Loaded">
                            <BeginStoryboard Storyboard="{StaticResource ToolTipOpenStoryboard}" />
                        </EventTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="DarkLightButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="Height" Value="60"/>
        <Setter Property="Width" Value="60"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <ControlTemplate.Resources>
                        <Storyboard x:Key="OnClickStoryboard">
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" Storyboard.TargetName="ButtonGrid">
                                <SplineDoubleKeyFrame KeyTime="0" Value="1" KeySpline="0.2,0,0.4,1"/>
                                <SplineDoubleKeyFrame KeyTime="0:0:0.2" Value="-1" KeySpline="0.2,0,0.4,1"/>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" Storyboard.TargetName="ButtonText">
                                <SplineDoubleKeyFrame KeyTime="0" Value="1" KeySpline="0.2,0,0.4,1"/>
                                <SplineDoubleKeyFrame KeyTime="0:0:0.2" Value="-1" KeySpline="0.2,0,0.4,1"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </ControlTemplate.Resources>
                    <Grid HorizontalAlignment="Center" VerticalAlignment="Center" x:Name="ButtonGrid" RenderTransformOrigin="0.5,0.5">
                        <Grid.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </Grid.RenderTransform>
                        <Ellipse Fill="{DynamicResource MaterialDesignPaper}" Width="{TemplateBinding Width}" Height="{TemplateBinding Height}">
                            <Ellipse.Effect>
                                <DropShadowEffect BlurRadius="8"
                                                  Direction="270"
                                                  Opacity=".42"
                                                  RenderingBias="Performance"
                                                  ShadowDepth="1.5"
                                                  Color="#AA000000" />
                            </Ellipse.Effect>
                        </Ellipse>
                        <TextBlock Foreground="{Binding RelativeSource={RelativeSource AncestorType={x:Type ctrl:ColorWheel}}, Path=CenterButtonFontColor}"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   x:Name="ButtonText"
                                   RenderTransformOrigin="0.5,0.5"
                                   Text="{Binding RelativeSource={RelativeSource AncestorType={x:Type ctrl:ColorWheel}}, Path=CenterButtonText}">
                            <TextBlock.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform/>
                                    <SkewTransform/>
                                    <RotateTransform/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </TextBlock.RenderTransform>
                        </TextBlock>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <EventTrigger RoutedEvent="Click">
                            <BeginStoryboard Storyboard="{StaticResource OnClickStoryboard}"/>
                        </EventTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
